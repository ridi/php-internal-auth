{
    "name": "ridibooks/internal-auth",
    "description": "Ridibooks Internal Auth",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ridibooks\\InternalAuth\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ridibooks\\Test\\InternalAuth\\": "tests"
        }
    },
    "require": {
        "web-token/jwt-core": "^2.1",
        "web-token/jwt-key-mgmt": "^2.1",
        "web-token/jwt-signature": "^2.1",
        "web-token/jwt-signature-algorithm-rsa": "^2.1",
        "web-token/jwt-checker": "^2.1",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.3",
        "silex/silex": "^2.3",
        "symfony/symfony": "^4.4",
        "guzzlehttp/guzzle": "^6.5"
    },
    "suggest": {
        "silex/silex": "Required when using `Ridibooks\\InternalAuth\\Silex\\Provider\\Authorizer`(compatible with 1.3.x versions)",
        "symfony/symfony": "Required when using `Ridibooks\\InternalAuth\\Authorization\\Authorizer`(compatible with 4.x.x versions)",
        "guzzlehttp/guzzle": "Required when using `Ridibooks\\InternalAuth\\Authorization\\Authorizer`"
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    }
}
